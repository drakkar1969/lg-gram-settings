<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!--// GramWidget template //-->
  <template class="GramWidget" parent="AdwActionRow">
    <property name="sensitive">false</property>
    <property name="use-underline">true</property>
    <property name="activatable">true</property>
    <child type="prefix">
      <object class="GtkImage" id="icon">
        <property name="pixel-size">24</property>
      </object>
    </child>
    <child type="suffix">
      <object class="AdwToggleGroup" id="feature_group">
        <property name="valign">center</property>
        <style>
          <class name="round"/>
        </style>
        <child>
          <object class="AdwToggle" id="off_toggle">
          </object>
        </child>
        <child>
          <object class="AdwToggle" id="on_toggle">
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
